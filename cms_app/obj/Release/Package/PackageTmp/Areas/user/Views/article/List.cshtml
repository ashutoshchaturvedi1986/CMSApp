@{
    ViewBag.Title = "Article's List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Breadcrumbs-->
<ol class="breadcrumb">
    <li class="breadcrumb-item">
        <a href="@Url.Action("index", "dashboard" , new { area= "admin" })">Dashboard</a>
    </li>
    <li class="breadcrumb-item active">Ordered Mold's List</li>
</ol>

<!-- Page Content -->
<div class="row">
    <div class="col-xl-12 col-sm-12 mb-3">
        <div class="card">
            <div class="card-header">
                <i class="fas fa-table"></i>
                Ordered Mold's List For Article Creation
            </div>
            <div class="card-body">
                <div id="viewPlaceHolder" class="table-responsive">
                </div>
            </div>
        </div>
    </div>
</div>

<script src="@Url.Content("~/Scripts/user/article.js")"></script>
<script type="text/javascript">
    $(document).ready(function () {
        showMoldListForArticleCreation('ArticleCreate', "_DisplayData_Mold", reporturl);
    })

    function CreateNewDeveloperCode(code) {
        var ur = saveurl.replace('SaveResult', 'Index');
        window.location.href = ur + "?code=" + code;
    }
</script>

