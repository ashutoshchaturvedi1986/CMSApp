
@{
    ViewBag.Title = "Activity Master's List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Breadcrumbs-->
<ol class="breadcrumb">
    <li class="breadcrumb-item">
        <a href="@Url.Action("index", "dashboard" , new { area= "admin" })">Dashboard</a>
    </li>
    <li class="breadcrumb-item active">Activity Master</li>
</ol>

<!-- Page Content -->
<div class="row">
    <div class="col-xl-10 col-sm-12">
        <h1>Activity List</h1>
    </div>
    <div class="col-xl-2 col-sm-12">
        <a class="btn btn-primary btn-block addbtn" href="@Url.Action("Index", "activity" , new { area= "admin" })">Add New</a>
    </div>
</div>

<!-- Icon Cards-->
<div class="row">
    <div class="col-xl-12 col-sm-12 mb-3">
        <div class="card">
            <div class="card-header">
                <i class="fas fa-table"></i>
                Added Activity Master's List
            </div>
            <div class="card-body">
                <div id="viewPlaceHolder" class="table-responsive">
                </div>
            </div>
        </div>
    </div>
</div>

